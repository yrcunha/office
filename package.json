{
  "name": "office",
  "version": "1.0.0",
  "description": "",
  "author": {
    "name": "Yuri Guimar√£es Rodrigues",
    "email": "yrcunha@gmail.com",
    "url": ""
  },
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "npm run services:up && next dev",
    "services:up": "docker compose -f __infrastructure__/provisioning/development/docker-compose.yaml up -d",
    "services:stop": "docker compose -f __infrastructure__/provisioning/development/docker-compose.yaml stop",
    "services:down": "docker compose -f __infrastructure__/provisioning/development/docker-compose.yaml down",
    "migrate:create": "node-pg-migrate -m __infrastructure__/migrations create",
    "migrate:up": "node-pg-migrate -m __infrastructure__/migrations --envPath .env.development up",
    "test": "jest --runInBand",
    "test:watch": "jest --watchAll --runInBand",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write ."
  },
  "engines": {
    "node": "20.x.x"
  },
  "dependencies": {
    "dotenv-expand": "12.0.1",
    "next": "15.0.1",
    "pg": "8.13.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/node": "22.9.0",
    "@types/pg": "8.11.10",
    "@types/react": "18.3.12",
    "jest": "29.7.0",
    "node-pg-migrate": "7.8.0",
    "prettier": "3.3.3",
    "ts-jest": "29.2.5",
    "typescript": "5.6.3"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "rootDir": ".",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
